{
    "name": "triptasoft/filament-indonesia-regions",
    "description": "This is my package filament-indonesia-regions",
    "keywords": [
        "triptasoft",
        "laravel",
        "filament-indonesia-regions"
    ],
    "homepage": "https://github.com/triptasoft/filament-indonesia-regions",
    "support": {
        "issues": "https://github.com/triptasoft/filament-indonesia-regions/issues",
        "source": "https://github.com/triptasoft/filament-indonesia-regions"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "hazbu",
            "email": "developer@triptasoft.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/forms": "^4.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.6"
    },
    "autoload": {
        "psr-4": {
            "Triptasoft\\FilamentIndonesiaRegions\\": "src/",
            "Triptasoft\\FilamentIndonesiaRegions\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Triptasoft\\FilamentIndonesiaRegions\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Triptasoft\\FilamentIndonesiaRegions\\FilamentIndonesiaRegionsServiceProvider"
            ],
            "aliases": {
                "FilamentIndonesiaRegions": "Triptasoft\\FilamentIndonesiaRegions\\Facades\\FilamentIndonesiaRegions"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
